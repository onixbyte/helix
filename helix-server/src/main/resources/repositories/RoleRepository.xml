<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onixbyte.helix.repository.RoleRepository">
    <select id="queryDefaultRole" resultType="com.onixbyte.helix.domain.entity.Role">
        SELECT id,
               name,
               code,
               sort,
               default_value,
               description,
               status,
               created_at,
               updated_at
        FROM roles
        WHERE default_value = TRUE
    </select>
</mapper>